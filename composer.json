{
    "name": "jdtsoftware/laravel-email-templates",
    "description": "Laravel 5 database driven email templating",
    "type": "library",
    "authors": [
        {
            "name": "Jon Braud",
            "email": "jon@jdtsoftware.co.uk"
        }
    ],
    "require": {
        "illuminate/contracts": "^5.4",
        "illuminate/database": "^5.4",
        "illuminate/mail": "^5.4.17",
        "tijsverkoyen/css-to-inline-styles": "^2.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "JDT\\LaravelEmailTemplates\\ServiceProvider"
            ],
            "aliases": {
                "EmailTemplate": "JDT\\LaravelEmailTemplates\\Facades\\EmailTemplates"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "JDT\\LaravelEmailTemplates\\": "src/"
        }
    }
}
